name: dockercompose1203895922610356935
services:
  sqlserver:
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: '@Admin123'
    hostname: sqlserver
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      app-network: null
    ports:
      - mode: ingress
        target: 1433
        published: "11433"
        protocol: tcp
    restart: always
  tarefas:
    build:
      context: C:\Users\vinci\source\repos\Lista-de-tarefas\Tarefas
      dockerfile: Tarefas/Dockerfile
    depends_on:
      sqlserver:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: tarefas
    networks:
      app-network: null
    ports:
      - mode: ingress
        target: 8080
        published: "5001"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\vinci\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\vinci\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  app-network:
    name: dockercompose1203895922610356935_app-network
    driver: bridge